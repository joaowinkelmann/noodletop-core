{
  "name": "noodletop-ws",
  "version": "0.0.1",
  "author": "joaowinkelmann",
  "description": "WebSocket tabletop manager",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "dev": "bun --watch ./src/index.ts",
    "build": "bun build ./src/index.ts --target=bun --outfile=./dist/websocket.ts --minify",
    "start": "bun run ./dist/websocket.ts"
  },
  "dependencies": {
    "bun": "^1.1.0"
  },
  "devDependencies": {
    "@types/bun": "^1.0.12",
    "@types/node": "^20.12.2"
  }
}
